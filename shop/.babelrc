{
	"presets": ["es2015", "react", "stage-0", "stage-1"],
	"env": {
		"development": {
			"plugins": [
				"transform-decorators-legacy",
				[
					"react-transform",
					{
						"transforms": [
							{
								"transform": "react-transform-catch-errors",
								"imports": [
									"react",
									"redbox-react"
								]
							}
						]
					}
				],
				[
					"import",
					{
						"style":true,
						"libraryName":"antd-mobile"
					}
				]
			]
		}
    }
}
